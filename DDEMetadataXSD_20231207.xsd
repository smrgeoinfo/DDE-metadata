      <xs:enumeration value="thematicGeologicMappingGlobalService"/>
      <xs:enumeration value="thematicGeologicalTimelineService"/>
      <xs:enumeration value="thematicDigitalGeologicalOccurrenceService"/>
      <xs:enumeration value="thematicDinosaurService"/>
      <xs:enumeration value="thematicGlobalNamingService"/>
      <xs:enumeration value="thematicGeomorphologyMappingService"/>
      <xs:enumeration value="thematicGeoscienceStandardService"/>
      <xs:enumeration value="thematicOtherService"/>
      <xs:enumeration value="vocabularyService"/>
      <xs:enumeration value="registryService"/>
      <xs:enumeration value="discoveryService"/>
      <xs:enumeration value="viewService"/>
      <xs:enumeration value="otherService"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SpatialRepresentationTypeCode">
    <xs:annotation>
      <xs:documentation>Codes that specify the method used to represent geographic information in
        the resource</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="vector"/>
      <xs:enumeration value="grid"/>
      <xs:enumeration value="textTable"/>
      <xs:enumeration value="tin"/>
      <xs:enumeration value="stereoModel"/>
